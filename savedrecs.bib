@article{ ISI:000230759000018,
Author = {Mair, W and Piper, MDW and Partridge, L},
Title = {{Calories do not explain extension of life span by dietary restriction in
   Drosophila}},
Journal = {{PLOS BIOLOGY}},
Year = {{2005}},
Volume = {{3}},
Number = {{7}},
Pages = {{1305-1311}},
Month = {{JUL}},
Abstract = {{Dietary restriction (DR) extends life span in diverse organisms,
   including mammals, and common mechanisms may be at work. DR is often
   known as calorie restriction, because it has been suggested that
   reduction of calories, rather than of particular nutrients in the diet,
   mediates extension of life span in rodents. We here demonstrate that
   extension of life span by DR in Drosophila is not attributable to the
   reduction in calorie intake. Reduction of either dietary yeast or sugar
   can reduce mortality and extend life span, but by an amount that is
   unrelated to the calorie content of the food, and with yeast having a
   much greater effect per calorie than does sugar. Calorie intake is
   therefore not the key factor in the reduction of mortality rate by DR in
   this species.}},
